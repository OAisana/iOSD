func isPrime(number: Int) -> Bool {
    if number < 2 {
        return false
        
    }else if number == 2{
        return true
    }
    
    for i in 2...Int(Double(number).squareRoot()+1) {
        if number % i == 0 {
            return false
        }
    }
    return true
}

for n in 1...100 {
    if isPrime(number: n) {
        print(n, terminator: " ")
    }
}


func cToF(c: Double) -> Double{
    return c * 9/5 + 32
}
func cToK(c: Double) -> Double{
    return c + 273.15
}
func fToC(f: Double) -> Double{
    return (f - 32) * 5/9
}
func fToK(f: Double) -> Double{
    return (f - 32) * 5/9 + 273.15
}
func kToC(k: Double) -> Double{
    return k - 273.15
}
func kToF(k: Double) -> Double{
    return (k - 273.15) * 9/5 + 32
}

func changetemp(t: Double, unit: String){
        switch unit.lowercased(){
        case "c":
            print("Celsius: \(t) C")
            print("Fahrenhei: \(cToF(c: t)) F")
            print("Kelvin: \(cToK(c: t)) K")
        case "f":
            print("Celsius: \(fToC(f: t)) C")
            print("Fahrenhei: \(t) F")
            print("Kelvin: \(fToK(f: t)) K")
        case "k":
            print("Celsius: \(kToC(k: t)) C")
            print("Fahrenhei: \(fToK(f: t)) F")
            print("Kelvin: \(t) K")
        default:
            print("wrong unit, try again.")
        
    }
}

changetemp(t: 23, unit: "c")

